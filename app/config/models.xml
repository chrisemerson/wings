<?xml version="1.0" encoding="utf-8"?>

<!-- If no plural is specified, it defaults to simply adding an 's' to the end of the model name
     If no table is specified, it defaults to an underscored, lowercased version of the plural name

          (Model Name)    (Plural)         (Table)
     e.g. ExampleModel => ExampleModels => example_models

     Example of use:

     <model name="ModelName" [plural="ModelNames"] [table="model_names"] /> -->

<models>
  <model name="User" />
</models>